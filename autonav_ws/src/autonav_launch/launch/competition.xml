<launch>
    <node pkg="autonav_state" exec="statemanager" />
    <node pkg="autonav_display" exec="primary_display" output="screen" />
    <node pkg="autonav_logging" exec="logging" />
    <node pkg="autonav_logging" exec="combined_logging" />
    <node pkg="autonav_serial" exec="camera.py" name="autonav_serial_camera" />
    <node pkg="autonav_serial" exec="imu.py" name="autonav_serial_imu" />
    <node pkg="autonav_serial" exec="serial_node.py" name="autonav_serial_core" />
    <node pkg="autonav_vision" exec="transformations.py" />
    <node pkg="autonav_vision" exec="expandify.py" />
    <node pkg="autonav_filters" exec="filters.py" />
    <node pkg="autonav_nav" exec="astar.py" />
    <node pkg="autonav_nav" exec="path_resolver.py" />
    <include file="$(find-pkg-share rosbridge_server)/launch/rosbridge_websocket_launch.xml" />
</launch>