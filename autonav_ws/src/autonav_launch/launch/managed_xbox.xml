<launch>
    <node pkg="joy" exec="joy_node" name="joy">
        <param name="autorepeat_rate" value="20.0" />
        <!-- <param name="coalesce_interval" value="0.05" /> -->
        <param name="deadzone" value="0.1" />
    </node>
    <node pkg="autonav_state" exec="statemanager" />
    <node pkg="autonav_display" exec="primary_display" output="screen" />
    <node pkg="autonav_logging" exec="logging" />
    <node pkg="autonav_logging" exec="combined_logging" />
    <node pkg="autonav_serial" exec="camera.py" />
    <node pkg="autonav_serial" exec="imu.py" />
    <node pkg="autonav_serial" exec="serial_node.py" />
    <node pkg="autonav_filters" exec="filters.py" />
    <node pkg="autonav_vision" exec="transformations.py" />
</launch>