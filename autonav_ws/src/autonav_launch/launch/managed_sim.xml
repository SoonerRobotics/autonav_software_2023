<launch>
    <node pkg="autonav_state" exec="statemanager" name="autonav_state">
        <param name="is_simulator" value="true" />
    </node>
    <node pkg="autonav_display" exec="primary_display" name="autonav_display" output="screen" />
    <node pkg="autonav_logging" exec="logging" name="autonav_logging" />
    <node pkg="autonav_manual" exec="steam.py" name="autonav_joy_steam" />
    <node pkg="autonav_manual" exec="steamremote" name="autonav_remote" />
    <include file="$(find-pkg-share rosbridge_server)/launch/rosbridge_websocket_launch.xml" />
</launch>